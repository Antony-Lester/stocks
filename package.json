{
  "devDependencies": {
    "@types/msgpack5": "^3.4.3",
    "@types/node": "^14.11.2",
    "@types/pg": "^8.10.2",
    "@types/pg-format": "^1.0.2",
    "@types/readable-stream": "^4.0.2",
    "@types/ws": "^8.5.5",
    "gts": "^3.1.1",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.0"
  },
  "scripts": {
    "build": "clear; npx tsc",
    "build-watch": "clear; npx tsc -w",
    "lint": "clear; npx prettier . --write; gts lint --fix",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "clear; gts fix",
    "prepare": "npm run lint",
    "pretest": "npm run compile",
    "posttest": "",
    "test": "npx tsc; clear; npx prettier . --write; gts lint --fix; clear; jest --runInBand --no-cache --forceExit",
    "test-watch": "jest --runInBand --watchAll --no-cache --forceExit",
    "test-clean": "jest --clearCache; clear",
    "test-coverage": "jest --coverage",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand --detectOpenHandles --no-cache --forceExit",
    "test-detect": "jest --detectOpenHandles",
    "seed": " node /src/db/run-seed.ts",
    "database-test": "psql -U antony -d stocks_test",
    "database": "psql -U antony -d stocks"
  },
  "dependencies": {
    "@alpacahq/alpaca-trade-api": "^3.0.1",
    "dotenv": "^16.3.1",
    "node-pg-format": "^1.3.5",
    "pg": "^8.11.1",
    "pg-format": "^1.0.4"
  }
}
