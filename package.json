{
  "devDependencies": {
    "@types/msgpack5": "^3.4.3",
    "@types/node": "^14.11.2",
    "@types/pg": "^8.10.2",
    "@types/pg-format": "^1.0.2",
    "@types/readable-stream": "^4.0.2",
    "@types/ws": "^8.5.5",
    "gts": "^3.1.1",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.0"
  },
  "scripts": {
    "prod-seed": " node /src/db/run-seed.ts",
    "prod-database": " env-cmd -f .env.prod; psql -U antony -d stocks",

    
    "build": " env-cmd -f .env.test; clear; npx tsc",
    "build-watch": " env-cmd -f .env.test; clear; npx tsc -w",
    "lint": " env-cmd -f .env.test; clear; npx prettier . --write; gts lint --fix",
    "clean": " env-cmd -f .env.test; gts clean",
    "compile": " env-cmd -f .env.test; tsc",
    "fix": " env-cmd -f .env.test; clear; gts fix",
    "prepare": " env-cmd -f .env.test; npm run lint",
    "pretest": " env-cmd -f .env.test; npm run compile",
    "posttest": "",


    "test": "npx tsc; clear; npx prettier . --write; gts lint --fix; clear; env-cmd -f .env.test; jest --runInBand --no-cache --forceExit",
    "test-watch": " env-cmd -f .env.test; jest --runInBand --watchAll --no-cache --forceExit",
    "test-clean": " env-cmd -f .env.test; jest --clearCache; clear",
    "test-coverage": " env-cmd -f .env.test; jest --coverage",
    "test-debug": " env-cmd -f .env.test; env-cmd -f .env.test; node --inspect-brk node_modules/.bin/jest --runInBand --detectOpenHandles --no-cache --forceExit",
    "test-detect": "env-cmd -f .env.test; jest --detectOpenHandles",
    "test-database": " env-cmd -f .env.test; psql -U antony -d stocks_test"
  },
  "dependencies": {
    "@alpacahq/alpaca-trade-api": "^3.0.1",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "moment": "^2.29.4",
    "moment-range": "^4.0.2",
    "node-pg-format": "^1.3.5",
    "pg": "^8.11.1",
    "pg-format": "^1.0.4"
  }
}
